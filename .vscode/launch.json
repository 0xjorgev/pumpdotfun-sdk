    {
        "name": "Debug FastAPI with Uvicorn",
        "type": "python",
        "request": "launch",
        "module": "uvicorn",
        "args": [
          "main:app",
          "--port",
          "5001",
          "--reload"
        ],
        "jinja": true,
        "env": {
